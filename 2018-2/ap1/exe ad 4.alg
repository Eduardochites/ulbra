algoritmo "semnome"
var

   l,rs,sp,mg:caracter
inicio
   escreval ("[a] se for rs")
   escreval ("[b] se for sp")
   escreval ("[c] se for mg")
   leia (l)
   escolha l;
        caso "rs"
             escreva ("gaucho")
        caso "sp"
             escreva ("paulista")
        caso "mg"
             escreva ("mineiro")
   Fimescolha
   escreva ("voce e ",l)
fimalgoritmo