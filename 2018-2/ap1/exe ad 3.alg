algoritmo "semnome"
var
      n1,n2,n3,s:inteiro
inicio
      Escreva (" digite um numero")
      leia (n1)
      Escreva (" digite o segundo numero")
      leia (n2)
      Escreva ("digite o terceiro numero")
      leia (n3)
      s <- n1+n2+n3
      se s >50 entao
         escreva ("o valor ",s," e maior que 50")
      senao
         escreva (s)
      FimSe
      
fimalgoritmo